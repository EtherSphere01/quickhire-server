{
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 150,
    "files.associations": {
        "*.prisma": "prisma",
        "*.schema": "prisma"
    },
    "[prisma]": {
        "editor.defaultFormatter": "Prisma.prisma",
        "editor.formatOnSave": true,
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        }
    }
}
